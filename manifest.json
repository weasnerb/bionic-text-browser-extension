{
  "manifest_version": 3,
  "name": "Bionic Text Extension",
  "version": "1.0",
  "description": "Enables/disables Bionic Text.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_title": "Bionic Text"
  },
  "background": {
    "service_worker": "./bionic-text-service-worker.js"
  }
}